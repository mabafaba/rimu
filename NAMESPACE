import(stats,graphics)
export( "%has%", "%hasonly%", as.mr,  as.ms,   
 indicator,  mean0, mr_collapse, mr_count, 
mr_diff, mr_infreq, mr_inorder, mr_inseq, mr_intersect, mr_na, 
mr_reorder, mr_union, ms_infreq, ms_inorder, ms_inscore, ms_inseq, mr_recode,
ms_na, ms_reorder, mtable,  sum0)

S3method("[",mr)
S3method("[",ms)
S3method(as.character,mr)
S3method(as.character,ms)
S3method(as.data.frame,mr)
S3method(as.logical,mr)
S3method(as.logical,ms)

S3method(as.mr,data.frame)
S3method(as.mr,default)
S3method(as.mr,factor)
S3method(as.mr,list)
S3method(as.mr,ms)
S3method(as.mr,mr)

S3method(as.ms,data.frame)
S3method(as.ms,default)
S3method(as.ms,ms)
S3method(as.ms,mr)

S3method(as.numeric,ms)

S3method(image,mr)
S3method(image,ms)
S3method(plot,mr)

S3method(levels,mr)
S3method(levels,ms)
S3method("levels<-",mr)
S3method("levels<-",ms)

S3method(length,mr)
S3method(length,ms)

S3method(print,mr)
S3method(print,ms)

S3method(stack,mr)
S3method(stack,ms)
